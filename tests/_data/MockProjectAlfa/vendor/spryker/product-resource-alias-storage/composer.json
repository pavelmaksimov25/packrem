{
    "name": "spryker/product-resource-alias-storage",
    "type": "library",
    "description": "ProductResourceAliasStorage module",
    "license": "proprietary",
    "require": {
        "php": ">=7.4",
        "spryker/event-behavior": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/product": "^6.0.0",
        "spryker/product-storage": "^1.15.0",
        "spryker/storage": "^3.4.0",
        "spryker/store": "^1.4.0",
        "spryker/synchronization": "^1.0.0",
        "spryker/synchronization-behavior": "^1.0.0",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/event": "*"
    },
    "suggest": {
        "spryker/event": "If you want to use Event plugins, minimum required version: 2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
