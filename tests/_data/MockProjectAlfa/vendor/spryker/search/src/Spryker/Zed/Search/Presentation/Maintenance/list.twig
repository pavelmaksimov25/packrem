{% extends '@Gui/Layout/layout.twig' %}

{% set widget_title = 'Search keys' %}

{% block head_title widget_title | trans %}

{% block content %}
    {% embed '@Gui/Partials/widget.twig' %}
        {% block widget_content %}
            {% if isInSearchLegacyMode %}
                {{ searchTable | raw }}
            {% else %}
                {% include '@Search/_partials/mode-warning-message.twig' %}
            {% endif %}
        {% endblock %}
    {% endembed %}
{% endblock %}
